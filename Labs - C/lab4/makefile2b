#make file of task2b
#format is target-name: target dependencies
#{-tab-}actions

# All Targets
all: task2b

# Tool invocations
# Executable "hello" depends on the files numbers.o and main.o and add.s.
task2b: start.o task2b.o util.o
	ld -m elf_i386 start.o task2b.o util.o -o task2b

task2b.o: task2b.c
	gcc -m32 -Wall -ansi -c -nostdlib -fno-stack-protector task2b.c -o task2b.o

util.o: util.c
	gcc -m32 -Wall -ansi -c -nostdlib -fno-stack-protector util.c -o util.o
	
start.o: start.s
	nasm -f elf start.s -o start.o

#tell make that "clean" is not a file name!
.PHONY: clean

#Clean the build directory
clean: 
	rm -f *.o task2b

